}

function checkLicence(){
    consoleLog("Running checkLicence");

    if(globalUserStatusLoaded){
        if(globalUserHasLicence){
            consoleLog("has licence!");
            if(globalUserIsSubscriptor){
                consoleLog("is subscriptor!");
                $("#subs_purchase_block").hide();
            } else{
                consoleLog("is NOT subscriptor!");
                $("#subs_purchase_block").show();
            }
            userhasLicence = "yes";
            $("#purchase_block").hide();
        } else{
            consoleLog("has not licence!");
            $("#purchase_block").show();
            $("#subs_purchase_block").hide();
            userhasLicence = "no"; 
        }
        bindEvents();
        //common_debug_console = false;
    } else{
        consoleLog("waiting for checkLicence");
        setTimeout(checkLicence, loopInterval);
    }
}

