<script>
    function deleteCookies() {
        console.log("Logging Out");
        $.cookie("vx.strategy", null, { path: '/', domain: '.vexels.com' });
        $.cookie("vx.token.local", null, { path: '/', domain: '.vexels.com' });
    }

    function doSearch() {

        var URL_PREFIX = "www";
        var SSL_PREFIX = "https://";
        var PURE_DOMAIN = "vexels.com/";

        //if(URL_PREFIX == "") URL_PREFIX = "www";
        if (SSL_PREFIX == "") SSL_PREFIX = "https://";
        if (PURE_DOMAIN == "") PURE_DOMAIN = "vexels.com/";



        var a = $("#autocompleteSearch").val();
        a = a.split(' ').join('-');
        if ("" != a) {
            var b = $("#autocompleteSearch").attr("data-lang");
            "en" == b ? b = URL_PREFIX : "pt" == b && (b = "br"), a = a.toLowerCase();
            var url = $("#filter-select").data("url") || "graphics";


            $.ajax({
                url: '/ajax/vexels-api/canonize-string/',
                type: 'post',
                async: true,
                data: { string: a },
                beforeSend: function (request) {
                    request.setRequestHeader("Content-Type", "application/x-www-form-urlencoded");
                },
                statusCode: {
                    404: function () {
                        consoleLog('Could not contact server.');
                    },
                    500: function () {
                        consoleLog('A server-side error has occurred.');
                    }
                },
                success: function (data) {
                    consoleLog(JSON.stringify(data));

                    if (data.error == "" || data.error == null) {
                        if (b != "") b += ".";
                        if (data.result != "") {
                            var c = SSL_PREFIX + b + PURE_DOMAIN + url + "/" + data.result;
                            window.location.href = c;
                        } else {
                            var c = SSL_PREFIX + b + PURE_DOMAIN + url + "/" + a;
                            window.location.href = c;
                        }
                        consoleLog(c);
                    } else {
                        //error
                    }
                },
                fail: function (error) {
                    console.info(JSON.stringify(error));
                }
            });
        }
    }

    var headerCtrl = {
        pos: 0,
        sliderTimer: null
    };

    $(document).ready(
        function () {
            showSlide(headerCtrl.pos++ % $('.in-header-slider.header-slider .slide').length);
            setSliderTimer();

            $('.in-header-slider .header-slider-controls .header-slider-btn').click(function (event) {
                clearInterval(headerCtrl.sliderTimer);
                headerCtrl.pos = $(event.target).attr('pos') - 1;
                setSliderTimer();
                showSlide(headerCtrl.pos);
            });

            $('.in-header-slider .v-header__f-text').mouseenter(
                function () {
                    clearInterval(headerCtrl.sliderTimer);
                }
            );

            $('.in-header-slider .v-header__f-text').mouseleave(
                function () {
                    setSliderTimer();
                }
            )
        }
    );

    function showSlide(pos) {
        $('.in-header-slider.header-slider .slide').removeClass('active-slide');
        $($('.in-header-slider.header-slider .slide')[pos]).addClass('active-slide');

        $('.in-header-slider .header-slider-controls .header-slider-btn').removeClass('slider-btn-active');
        $($('.in-header-slider .header-slider-controls .header-slider-btn')[pos]).addClass('slider-btn-active');
    }

    function setSliderTimer() {
        headerCtrl.sliderTimer = window.setInterval(function () {
            showSlide(headerCtrl.pos++ % $('.in-header-slider.header-slider .slide').length);
        }, 4000);
    }

</script>

