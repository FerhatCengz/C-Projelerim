    function checkDownload(){
        var dateObj = new Date();
        var month = dateObj.getUTCMonth() + 1; //months from 1-12
        var day = dateObj.getUTCDate();
        var year = dateObj.getUTCFullYear();
        var dateToday = year + "/" + month + "/" + day;
        
        if (localStorage.getItem("downloadedVectors") === null) {
            localStorage.setItem("downloadLimitDate", dateToday);
            downloadLimitDate = localStorage.getItem("downloadLimitDate");
            localStorage.setItem("downloadedVectors", 0);
        } else {
            downloadLimitDate = localStorage.getItem("downloadLimitDate");
            if (dateToday === downloadLimitDate){
                downloadedVectors = localStorage.getItem("downloadedVectors");
            } else {
                localStorage.setItem("downloadedVectors", 0);
                localStorage.setItem("downloadLimitDate", dateToday);
                downloadedVectors = 0;
            }
        }
